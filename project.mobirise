          "_customHTML": "<section class=\"form3 hotelm4_form3\" group=\"Forms\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"bootstrap-datepicker, mbr-booking-datepicker\">\n\n\t<mbr-parameters>\n\t\t\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"3\">\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Underline\" name=\"showUnderline\" condition=\"showSubtitle\" checked>\n\n\t\t<input type=\"checkbox\" title=\"First Item\" name=\"showFirstItem\">\n\n\t\t<input type=\"checkbox\" title=\"1st Item Text\" name=\"showFirstText\" condition=\"showFirstItem\" checked>\n\t\t<input type=\"checkbox\" title=\"1st Item Button\" name=\"showFirstButton\" checked condition=\"showFirstItem\">\n\t\t<input type=\"checkbox\" title=\"Second Item\" name=\"showSecondItem\">\n\t\t<input type=\"checkbox\" title=\"Line\" name=\"showLine\" checked condition=\"showSecondItem\">\n\t\t<input type=\"checkbox\" title=\"Items Title\" name=\"showItemsTitle\" condition=\"showFirstItem || showSecondItem\" checked>\n\t\t<input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"(showFirstItem || showSecondItem) && showForm\">\n\t\t<input type=\"checkbox\" title=\"Form\" name=\"showForm\">\n\t\t<header condition=\"(showUnderline && showSubtitle) || showFirstItem || showSecondItem || (showLine && showSecondItem)\">\n\t\t\tColors</header>\n\t\t<input type=\"color\" title=\"Underline\" name=\"underlineColor\" value=\"#ffffff\" condition=\"showUnderline && showSubtitle\">\n\t\t<input type=\"color\" title=\"1st Item BG\" name=\"firstBg\" value=\"#ea7368\" condition=\"showFirstItem\">\n\t\t<input type=\"color\" title=\"2nd Item BG\" name=\"secondBg\" value=\"#162315\" condition=\"showSecondItem\">\n\t\t<input type=\"color\" title=\"Line\" name=\"lineColor\" value=\"#000000\" condition=\"showLine && showSecondItem\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background6-1.jpg\">\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#000000\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=2jqUN6d6ZyM\">\n\t\t</fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1-1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n\n\t\t<header condition=\"showForm\">Form Settings</header>\n\t\t<input type=\"checkbox\" title=\"Show Labels\" name=\"showLabels\" checked condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Date Color\" name=\"dateColor\" value=\"#000000\" condition=\"showForm\">\n\t\t<input type=\"text\" inline title=\"Min Date (mm/dd/yyyy)\" name=\"startDate\" value=\"01/01/2020\" condition=\"showForm\">\n\t\t<input type=\"text\" inline title=\"Max Date (mm/dd/yyyy)\" name=\"endDate\" value=\"02/01/2025\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Datepicker BG Color\" name=\"calendarBg\" value=\"#ffffff\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Datepicker Text Color\" name=\"calendarText\" value=\"#000000\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Active Day Color\" name=\"activeColor\" value=\"#2b6477\" condition=\"showForm\">\n\t\t<input type=\"color\" title=\"Active Day Text Color\" name=\"activeTextColor\" value=\"#ffffff\" condition=\"showForm\">\n\t\t<input type=\"checkbox\" title=\"Show Lines\" name=\"showLines\" condition=\"showForm\" checked>\n\t\t<input type=\"color\" title=\"Lines Color\" name=\"linesColor\" value=\"#234a3a\" condition=\"showLines && showForm\">\n\t\t<input type=\"color\" title=\"BG Color\" name=\"formBg\" value=\"#fcfcfc\" condition=\"showForm\">\n\t\t\n\t</mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-lighter mbr-white align-center\" mbr-class=\"{'pb-3': !showUnderline}\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle, .underline\">RESERVAS A PARTIR DE</h3>\n\t\t<div class=\"underline pb-3 align-center\" mbr-if=\"showUnderline && showSubtitle\">\n\t\t\t<div class=\"line\"></div>\n\t\t</div>\n\t\t<h2 class=\"mbr-section-title main-title mbr-fonts-style mbr-white align-center\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".main-title\">\n\t\t\tFebrero 23 - 2024</h2>\n\t\t<div class=\"row justify-content-center\">\n\t\t\t<div class=\"content-wrap col-12\" mbr-class=\"{'pt-5': showTitle || showSubtitle}\" mbr-if=\"showFirstItem || showSecondItem || showForm\">\n\t\t\t\t<div class=\"first-item mbr-white\" mbr-class=\"{'col-lg-3': showSecondItem && showForm,\n                                'col-lg-4': !showSecondItem || !showForm}\" mbr-if=\"showFirstItem\">\n\t\t\t\t\t